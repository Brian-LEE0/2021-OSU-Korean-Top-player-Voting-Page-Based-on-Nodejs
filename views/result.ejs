<!DOCTYPE html>
<html>

<head>
    <title>List Page</title>
</head>

<body>
    <h1>List Page</h1>
    <h5>db1 :
        <%=data.db1.db1%>
    </h5>
    <h5>db2 :
        <%=data.db2.db2%>
    </h5>
    <hr />
    <table width="100%" border="1">
        <tr>
            <th>RANK</th>
            <th>NAME</th>
            <th>ID</th>
            <th>POINT</th>
            <th>COUNT</th>
        </tr>
        <% data.forEach(function (item, index) { %>
            <tr>
                <td>
                    <%= item.ranking %>
                </td>
                <td>
                    <%= item.name %>
                </td>
                <td>
                    <%= item.candidate_id %>
                </td>
                <td>
                    <%= item.pt %>
                </td>
                <td>
                    <%= item._count %>
                </td>
            </tr>
            <% }); %>
    </table>
</body>

</html>