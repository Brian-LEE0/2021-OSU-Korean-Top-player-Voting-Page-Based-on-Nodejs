<!DOCTYPE html>
<html>

<head>
    <title>List Page</title>
</head>

<body>
    <h1>List Page</h1>
    <hr />
    <table width="100%" border="1">
        <tr>
            <th>DELETE</th>
            <th>EDIT</th>
            <th>ID</th>
            <th>Name</th>
            <th>Model Number</th>
            <th>Series</th>
        </tr>
        <% data.forEach(function (item, index) { %>
            <tr>
                <td>
                    <%= item.ranking %>
                </td>
                <td>
                    <%= item.name %>
                </td>
                <td>
                    <%= item.candidate_id %>
                </td>
                <td>
                    <%= item.pt %>
                </td>
                <td>
                    <%= item._count %>
                </td>
            </tr>
            <% }); %>
    </table>
</body>

</html>